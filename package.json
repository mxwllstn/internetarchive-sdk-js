{
  "name": "internetarchive-sdk-js",
  "version": "1.0.36",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "author": "Max Stein <maxwell.stein@gmail.com> (https://maxstein.net)",
  "license": "MIT",
  "description": "NodeJS / Typescript SDK for Internet Archive APIs",
  "keywords": [
    "archive",
    "internet archive",
    "archive.org",
    "sdk",
    "api",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build-clean && npm run build-esm && npm run build-cjs",
    "build-clean": "rm -rf dist",
    "build-esm": "npx tsc",
    "build-cjs": "npx rollup dist/index.js --file dist/index.cjs -p @rollup/plugin-json --format cjs --external fs,crypto,slugify,qs,zod,pkg-types",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint --fix .",
    "test": "vitest run",
    "prepublish": "npm run lint && npm run build",
    "release": "npm run lint && npm run build && npm run test && changelogen --release --push && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mxwllstn/internetarchive-sdk-js"
  },
  "dependencies": {
    "pkg-types": "^1.1.3",
    "qs": "^6.12.1",
    "slugify": "^1.6.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@rollup/plugin-json": "^6.1.0",
    "@stylistic/eslint-plugin": "^2.3.0",
    "@types/node": "^20.14.9",
    "@types/qs": "^6.9.15",
    "changelogen": "^0.5.5",
    "eslint": "^8.57.0",
    "globals": "^15.6.0",
    "lint-staged": "^15.2.7",
    "rollup": "^4.18.0",
    "tsx": "^4.15.7",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.14.1",
    "vitest": "^2.0.3"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@9.3.0+sha512.ee7b93e0c2bd11409c6424f92b866f31d3ea1bef5fbe47d3c7500cdc3c9668833d2e55681ad66df5b640c61fa9dc25d546efa54d76d7f8bf54b13614ac293631"
}