{
  "name": "internetarchive-api-wrapper",
  "version": "1.0.0",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "NODE_PATH=./ ts-node-dev --respawn --require dotenv/config server.ts",
    "start:dev": "NODE_PATH=./ ts-node-dev --inspect=4321 --respawn --require dotenv/config server.ts",
    "lint": "eslint --fix ./server.ts",
    "postinstall": "yarn build"
  },
  "dependencies": {
    "@types/node": "^16.7.10",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "dotenv": "^10.0.0",
    "typescript": "^4.3.2",
    "express": "^4.17.1",
    "internetarchive-api-wrapper": "git+ssh://git@github.com:mxwllstn/internetarchive-api-wrapper",
    "morgan-body": "^2.6.6",
    "multer": "^1.4.3"
  },
  "devDependencies": {
    "eslint": "^7.10.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.1.1",
    "ts-node-dev": "^1.1.6"
  },
  "engines": {
    "node": "^14.17"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.6.1",
    "**/@typescript-eslint/parser": "^4.6.1"
  }
}
