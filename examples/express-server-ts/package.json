{
  "name": "internetarchive-sdk-ts-example",
  "version": "1.0.0",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc",
    "start": "yarn build && node dist/server.js",
    "start:dev": "NODE_PATH=./ ts-node-dev --respawn --require dotenv/config server.ts",
    "lint": "eslint --fix ./server.ts",
    "postinstall": "yarn build"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "internetarchive-sdk-js": "^1.0.17-beta",
    "morgan-body": "^2.6.9",
    "multer": "^1.4.5-lts.1",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "lint-staged": "^15.2.5",
    "prettier": "^3.3.1",
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": ">=14"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
