{
  "name": "internetarchive-sdk-ts-example",
  "version": "1.0.0",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc",
    "start": "yarn build && node dist/server.js",
    "start:dev": "NODE_PATH=./ ts-node-dev --respawn --require dotenv/config server.ts",
    "lint": "eslint --fix ./server.ts",
    "postinstall": "yarn build"
  },
  "dependencies": {
    "internetarchive-sdk-js": "^1.0.11-beta",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.0",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "morgan-body": "^2.6.9",
    "multer": "^1.4.5-lts.1",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "eslint": "^8.42.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": ">=12"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
